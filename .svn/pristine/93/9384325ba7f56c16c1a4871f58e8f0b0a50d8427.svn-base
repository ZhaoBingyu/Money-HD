import {ajaxSubmit} from './util';

// 权限信息管理:增加按钮
function addBtn(obj) {
  const sendUrl = this.url.commonUrl + '/shiro/addBtn';
  const sendData: any = obj;
  return ajaxSubmit(sendUrl, sendData);
}

